  \lineto[2*\@headlength/\@headwidth,0]%
  \lineto[\@headlength+\sin(.5*\@hangle),-(.5*\@headwidth-\cos(.5*\@hangle))]%
  \lineto[\@headlength,-.5*\@headwidth]%
