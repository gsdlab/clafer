from jsir.IR import *

c0_AliceAge = Clafer("c0_AliceAge").withCard(1, 1);
c0_BobAge = Clafer("c0_BobAge").withCard(1, 1);
c0_BobsBirthday = Clafer("c0_BobsBirthday").withCard(0, 1);
c0_AliceAge.refTo("int");
c0_BobAge.refTo("int");
Constraint(equal(ifThenElse(some(glob(c0_BobsBirthday)), joinRef(glob(c0_BobAge)), joinRef(glob(c0_AliceAge))), constant(5)));
defaultScope(1);
stringLength(16);
