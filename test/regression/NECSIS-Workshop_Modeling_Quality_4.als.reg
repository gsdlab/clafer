open util/integer
pred show {}


one sig c1_aCar extends c18_Car
{}

abstract sig c18_Car
{ r_c19_ABS : lone c19_ABS
, r_c29_Transmission : one c29_Transmission
, r_c62_FCA : lone c62_FCA
, r_c123_CC : lone c123_CC
, r_c154_total_cost : one c154_total_cost
, r_c161_total_comfort : one c161_total_comfort
, r_c168_total_fuel : one c168_total_fuel
, r_c175_total_mass : one c175_total_mass }
{ (some (this.@r_c123_CC).@r_c136_ACC) => (some this.@r_c62_FCA)
  (this.(@r_c154_total_cost.@ref)) = (sum temp : (c182_Feature.@r_c183_cost) | temp.@ref)
  (this.(@r_c161_total_comfort.@ref)) = (sum temp : (c185_ComfortFeature.@r_c183_cost) | temp.@ref)
  (this.(@r_c168_total_fuel.@ref)) = (sum temp : (c187_FuelFeature.@r_c183_cost) | temp.@ref)
  (this.(@r_c175_total_mass.@ref)) = (sum temp : (c187_FuelFeature.@r_c184_mass) | temp.@ref) }

sig c19_ABS extends c185_ComfortFeature
{}
{ one @r_c19_ABS.this
  (this.(@r_c183_cost.@ref)) = 2
  (this.(@r_c184_mass.@ref)) = 1
  (this.(@r_c186_comfort.@ref)) = 1 }

sig c29_Transmission extends c182_Feature
{ r_c36_Automatic : lone c36_Automatic
, r_c49_Manual : lone c49_Manual }
{ one @r_c29_Transmission.this
  let children = (r_c36_Automatic + r_c49_Manual) | one children
  (this.(@r_c183_cost.@ref)) = 0
  (this.(@r_c184_mass.@ref)) = 0 }

sig c36_Automatic extends c187_FuelFeature
