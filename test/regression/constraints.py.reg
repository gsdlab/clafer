from jsir.IR import *

c0_Phone = Abstract("c0_Phone");
c0_networking = c0_Phone.addChild("c0_networking").withCard(1, 1);
c0_wifi = c0_networking.addChild("c0_wifi").withCard(0, 1);
c0_typeB = c0_wifi.addChild("c0_typeB").withCard(0, 1);
c0_typeG = c0_wifi.addChild("c0_typeG").withCard(0, 1);
c0_typeN = c0_wifi.addChild("c0_typeN").withCard(0, 1);
c0_bluetooth = c0_networking.addChild("c0_bluetooth").withCard(0, 1);
c0_mobile = c0_networking.addChild("c0_mobile").withCard(0, 1);
c0_nfc = c0_networking.addChild("c0_nfc").withCard(0, 1);
c0_location = c0_Phone.addChild("c0_location").withCard(0, 1).withGroupCard(1, 1);
c0_gps = c0_location.addChild("c0_gps").withCard(0, 1);
c0_agps = c0_location.addChild("c0_agps").withCard(0, 1);
c0_gpu = c0_Phone.addChild("c0_gpu");
c0_PhoneWithGps = Clafer("c0_PhoneWithGps").withCard(1, 1).extending(c0_Phone);
c0_PhoneWithWifi = Clafer("c0_PhoneWithWifi").withCard(1, 1).extending(c0_Phone);
c0_PhonewithWifiAndGpsNoBluetoothOrNfc = Clafer("c0_PhonewithWifiAndGpsNoBluetoothOrNfc").withCard(1, 1).extending(c0_Phone);
c0_PhoneWithGpu = Clafer("c0_PhoneWithGpu").withCard(1, 1).extending(c0_Phone);
Constraint(all([decl([p = local("p")], glob(c0_Phone))], some(join(join(p, c0_networking), c0_mobile))));
c0_PhoneWithGps.addConstraint(some(join(join($this(), c0_location), c0_gps)));
c0_PhoneWithWifi.addConstraint(some(join(join(join($this(), c0_networking), c0_wifi), c0_typeN)));
c0_PhonewithWifiAndGpsNoBluetoothOrNfc.addConstraint(and(and(some(join(join($this(), c0_networking), c0_wifi)), some(join(join($this(), c0_location), c0_gps))), not(or(some(join(join($this(), c0_networking), c0_bluetooth)), some(join(join($this(), c0_networking), c0_nfc))))));
c0_PhoneWithGpu.addConstraint(some(join($this(), c0_gpu)));
scope({c0_Phone:4, c0_agps:4, c0_bluetooth:4, c0_gps:4, c0_gpu:4, c0_location:4, c0_mobile:4, c0_networking:4, c0_nfc:4, c0_typeB:4, c0_typeG:4, c0_typeN:4, c0_wifi:4});
defaultScope(1);
stringLength(16);
