from jsir.IR import *

c0_Bluetooth = Abstract("c0_Bluetooth");
c0_MobilePhone = Abstract("c0_MobilePhone");
c0_LowEndPhone = Abstract("c0_LowEndPhone").extending(c0_MobilePhone);
c0_HighEndPhone = Abstract("c0_HighEndPhone").extending(c0_MobilePhone);
c0_Bluetooth20 = c0_Bluetooth.addChild("c0_Bluetooth20").withCard(0, 1);
c0_Bluetooth21 = c0_Bluetooth.addChild("c0_Bluetooth21").withCard(0, 1);
c0_Bluetooth40 = c0_Bluetooth.addChild("c0_Bluetooth40").withCard(0, 1);
c0_Connectivity = c0_MobilePhone.addChild("c0_Connectivity").withCard(1, 1);
c1_Bluetooth = c0_Connectivity.addChild("c1_Bluetooth").withCard(1, 1).withGroupCard(1, 1).extending(c0_Bluetooth);
c0_GSM = c0_Connectivity.addChild("c0_GSM").withCard(1, 1);
c0_LTE = c0_Connectivity.addChild("c0_LTE").withCard(0, 1);
c0_WiFi = c0_Connectivity.addChild("c0_WiFi").withCard(0, 1);
c0_USB = c0_Connectivity.addChild("c0_USB").withCard(0, 1);
c0_Battery = c0_MobilePhone.addChild("c0_Battery").withCard(1, 1).withGroupCard(1, 1);
c0_LiBattery1150 = c0_Battery.addChild("c0_LiBattery1150").withCard(0, 1);
c0_LiBattery1400 = c0_Battery.addChild("c0_LiBattery1400").withCard(0, 1);
c0_QuadCPU = c0_HighEndPhone.addChild("c0_QuadCPU").withCard(0, 1);
c0_Phone1 = Clafer("c0_Phone1").withCard(1, 1).extending(c0_MobilePhone);
c0_Phone2 = Clafer("c0_Phone2").withCard(1, 1).extending(c0_HighEndPhone);
c0_LTE.addConstraint(some(join(join(joinParent(joinParent($this())), c0_Battery), c0_LiBattery1400)));
c0_LowEndPhone.addConstraint(none(join(join($this(), c0_Connectivity), c0_WiFi)));
c0_HighEndPhone.addConstraint(and(some(join(join($this(), c0_Connectivity), c0_USB)), some(join(join($this(), c0_Connectivity), c0_WiFi))));
c0_Phone1.addConstraint(some(join(join($this(), c0_Connectivity), c0_WiFi)));
c0_Phone1.addConstraint(some(join(join($this(), c0_Connectivity), c0_LTE)));
c0_Phone1.addConstraint(some(join(join(join($this(), c0_Connectivity), c1_Bluetooth), c0_Bluetooth21)));
c0_Phone2.addConstraint(some(join(join($this(), c0_Connectivity), c0_LTE)));
c0_Phone2.addConstraint(some(join(join(join($this(), c0_Connectivity), c1_Bluetooth), c0_Bluetooth21)));
c0_Phone2.addConstraint(some(join($this(), c0_QuadCPU)));
scope({c0_Battery:3, c0_Bluetooth:3, c0_Bluetooth20:3, c0_Bluetooth21:3, c0_Bluetooth40:3, c0_Connectivity:3, c0_GSM:3, c0_LTE:3, c0_LiBattery1150:3, c0_LiBattery1400:3, c0_LowEndPhone:0, c0_MobilePhone:3, c0_USB:3, c0_WiFi:3, c1_Bluetooth:3});
defaultScope(1);
stringLength(16);
