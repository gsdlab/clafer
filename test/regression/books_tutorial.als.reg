open util/integer
pred show {}


abstract sig c0_Book
{ r_c0_title : one c0_title
, r_c0_year : one c0_year
, r_c0_page : set c0_page
, r_c0_format : one c0_format
, r_c0_kind : one c0_kind
, r_c0_authors : some c0_authors
, r_c0_ISBN : lone c0_ISBN }
{ 2 <= #r_c0_page
  all disj x, y : this.@r_c0_authors | (x.@ref) != (y.@ref) 
  ((this.(@r_c0_year.@ref)) >= 5) => (some this.@r_c0_ISBN) }

sig c0_title
{ ref : one Int
, r_c0_subtitle : lone c0_subtitle }
{ one @r_c0_title.this }

sig c0_subtitle
{ ref : one Int }
{ one @r_c0_subtitle.this }

sig c0_year
{ ref : one Int }
{ one @r_c0_year.this }

sig c0_page
{}
{ one @r_c0_page.this }

sig c0_format
{ r_c0_paper : lone c0_paper
, r_c0_electronic : lone c0_electronic }
{ one @r_c0_format.this
  let children = (r_c0_paper + r_c0_electronic) | one children }

sig c0_paper
{ r_c0_hardcover : lone c0_hardcover }
{ one @r_c0_paper.this }

sig c0_hardcover
{}
{ one @r_c0_hardcover.this }

sig c0_electronic
{}
{ one @r_c0_electronic.this }

sig c0_kind
{ r_c0_textbook : lone c0_textbook
, r_c0_manual : lone c0_manual
, r_c0_reference : lone c0_reference
, r_c0_fiction : lone c0_fiction
, r_c0_nonfiction : lone c0_nonfiction
, r_c0_other : lone c0_other }
{ one @r_c0_kind.this
  let children = (r_c0_textbook + r_c0_manual + r_c0_reference + r_c0_fiction + r_c0_nonfiction + r_c0_other) | one children }

sig c0_textbook
{}
{ one @r_c0_textbook.this }

sig c0_manual
{}
{ one @r_c0_manual.this }

sig c0_reference
{}
{ one @r_c0_reference.this }

sig c0_fiction
{}
{ one @r_c0_fiction.this }

sig c0_nonfiction
{}
{ one @r_c0_nonfiction.this }

sig c0_other
{ ref : one Int }
{ one @r_c0_other.this }

sig c0_authors
{ ref : one c0_Author }
{ one @r_c0_authors.this }

sig c0_ISBN
{ ref : one Int
, r_c0_GS1 : lone c0_GS1 }
{ one @r_c0_ISBN.this
  (((this.~@r_c0_ISBN).(@r_c0_year.@ref)) >= 6) => (some this.@r_c0_GS1) }

sig c0_GS1
{ ref : one Int }
{ one @r_c0_GS1.this }

abstract sig c0_Person
{ r_c0_name : one c0_name
, r_c0_dob : lone c0_dob }

sig c0_name
{ ref : one Int }
{ one @r_c0_name.this }

sig c0_dob
{ ref : one Int }
{ one @r_c0_dob.this }

abstract sig c0_Author extends c0_Person
{ r_c0_books : some c0_books }
{ all disj x, y : this.@r_c0_books | (x.@ref) != (y.@ref)  }

sig c0_books
{ ref : one c0_Book }
{ one @r_c0_books.this }

one sig c0_GenerativeProgramming extends c0_Book
{}
{ (((((((((this.(@r_c0_title.@ref)) = 0) && (no (this.@r_c0_title).@r_c0_subtitle)) && ((this.(@r_c0_year.@ref)) = 5)) && ((#(this.@r_c0_page)) = 4)) && (some (this.@r_c0_format).@r_c0_paper)) && (some (this.@r_c0_kind).@r_c0_nonfiction)) && ((this.(@r_c0_authors.@ref)) = (c0_Czarnecki + c0_Eisenecker))) && ((this.(@r_c0_ISBN.@ref)) = 0)) && (no (this.@r_c0_ISBN).@r_c0_GS1) }

one sig c0_Czarnecki extends c0_Author
{}
{ ((this.(@r_c0_name.@ref)) = 0) && (c0_GenerativeProgramming in (this.(@r_c0_books.@ref))) }

one sig c0_Eisenecker extends c0_Author
{}
{ ((this.(@r_c0_name.@ref)) = 0) && (c0_GenerativeProgramming in (this.(@r_c0_books.@ref))) }

