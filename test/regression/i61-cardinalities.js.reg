defaultScope(1);
intRange(-8, 7);
stringLength(16);

c0_A = Clafer("c0_A").withCard(1, 1).withGroupCard(2, 3);
c0_b = c0_A.addChild("c0_b").withCard(0, 1);
c0_c = c0_A.addChild("c0_c").withCard(1, 1);
c0_d = c0_A.addChild("c0_d").withCard(0, 1);
c0_E = Clafer("c0_E").withCard(1, 1).withGroupCard(1, 1);
c0_f = c0_E.addChild("c0_f").withCard(0, 1);
c0_g = c0_E.addChild("c0_g").withCard(0, 1);
c0_D = Clafer("c0_D").withCard(1, 1);
c0_Z = c0_D.addChild("c0_Z").withCard(1, 1);
c0_K = c0_D.addChild("c0_K").withCard(0, 1);
c0_L = Clafer("c0_L").withCard(1, 1);
c0_M = c0_L.addChild("c0_M").withCard(1, 1);
c0_N = c0_L.addChild("c0_N").withCard(1, 1);
assert(or(some(join(global(c0_E), c0_f)), some(join(global(c0_E), c0_g))));
assert(not(and(some(join(global(c0_E), c0_f)), some(join(global(c0_E), c0_g)))));
assert(one(global(c0_D)));
assert(one(join(global(c0_D), c0_Z)));
