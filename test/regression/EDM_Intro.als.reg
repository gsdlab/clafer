one sig c0_SueAgenda extends c0_Agenda
{ r_c0_m1 : lone c0_m1
, r_c0_m2 : lone c0_m2 }
{ (some this.@r_c0_m1) => ((this.(@r_c0_m1.@c0_m1_ref)) = c0_M1)
  (some this.@r_c0_m2) => ((this.(@r_c0_m2.@c0_m2_ref)) = c0_M2) }

