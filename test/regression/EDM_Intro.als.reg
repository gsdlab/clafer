  all disj x, y : this.@r_c0_m1 | (x.@ref) != (y.@ref)
  (some this.@r_c0_m2) => ((this.(@r_c0_m2.@ref)) = c0_M2)
  all disj x, y : this.@r_c0_m2 | (x.@ref) != (y.@ref) }

