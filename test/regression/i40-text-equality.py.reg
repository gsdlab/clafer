from jsir.IR import *

c0_text = Clafer("c0_text").withCard(1, 1);
c0_text1 = Clafer("c0_text1").withCard(1, 1);
c0_text2 = Clafer("c0_text2").withCard(1, 1);
c0_equal1 = Clafer("c0_equal1").withCard(0, 1);
c0_equal2 = Clafer("c0_equal2").withCard(0, 1);
c0_text.refTo(string);
c0_text1.refTo(string);
c0_text2.refTo(string);
Constraint(implies(some(glob(c0_text)), equal(joinRef(glob(c0_text)), constant("\"some text\""))));
Constraint(implies(some(glob(c0_text1)), equal(joinRef(glob(c0_text1)), constant("\"some text\""))));
Constraint(implies(some(glob(c0_text2)), equal(joinRef(glob(c0_text2)), constant("\"some text\""))));
Constraint(some(glob(c0_equal1)));
Constraint(some(glob(c0_equal2)));
c0_equal1.addConstraint(equal(joinRef(glob(c0_text)), joinRef(glob(c0_text1))));
c0_equal2.addConstraint(equal(joinRef(glob(c0_text1)), joinRef(glob(c0_text2))));
defaultScope(1);
stringLength(16);
