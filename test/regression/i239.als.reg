open util/integer
pred show {}


fact { 2 <= #c0_x }
sig c0_x
{ c0_x_ref : one Int }
{ ((this.@c0_x_ref) >= (-1.mul[2])) && ((this.@c0_x_ref) <= 2) }

fact { 2 <= #c0_y }
sig c0_y
{ c0_y_ref : one Int }
{ ((this.@c0_y_ref) >= (-1.mul[2])) && ((this.@c0_y_ref) <= 2) }

one sig c0_z
{ c0_z_ref : one Int }

fact { (some c0_z) => ((c0_z.@c0_z_ref) = (sum temp : (c0_x + c0_y) | temp.(@c0_y_ref + @c0_x_ref))) }
assert assertOnLine_6 { (c0_z.@c0_z_ref) = ((sum temp : c0_x | temp.@c0_x_ref).plus[(sum temp : c0_y | temp.@c0_y_ref)]) }
check assertOnLine_6 for 1 but 2 c0_x, 2 c0_y
