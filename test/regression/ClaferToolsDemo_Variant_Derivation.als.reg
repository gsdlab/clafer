open util/integer
pred show {}


abstract sig c1_Bluetooth
{ r_c2_Bluetooth20 : lone c2_Bluetooth20
, r_c3_Bluetooth21 : lone c3_Bluetooth21
, r_c4_Bluetooth40 : lone c4_Bluetooth40 }
{ let children = (r_c2_Bluetooth20 + r_c3_Bluetooth21 + r_c4_Bluetooth40) | one children }

sig c2_Bluetooth20
{}
{ one @r_c2_Bluetooth20.this }

sig c3_Bluetooth21
{}
{ one @r_c3_Bluetooth21.this }

sig c4_Bluetooth40
{}
{ one @r_c4_Bluetooth40.this }

abstract sig c5_MobilePhone
{ r_c6_Connectivity : one c6_Connectivity
, r_c14_Battery : one c14_Battery }

sig c6_Connectivity
{ r_c7_Bluetooth : one c7_Bluetooth
, r_c8_GSM : one c8_GSM
, r_c9_LTE : lone c9_LTE
, r_c12_WiFi : lone c12_WiFi
, r_c13_USB : lone c13_USB }
{ one @r_c6_Connectivity.this }

sig c7_Bluetooth extends c1_Bluetooth
{}
{ one @r_c7_Bluetooth.this }

sig c8_GSM
{}
{ one @r_c8_GSM.this }

sig c9_LTE
{}
{ one @r_c9_LTE.this
  some (((this.~@r_c9_LTE).~@r_c6_Connectivity).@r_c14_Battery).@r_c16_LiBattery1400 }

sig c12_WiFi
{}
{ one @r_c12_WiFi.this }

sig c13_USB
{}
{ one @r_c13_USB.this }

sig c14_Battery
{ r_c15_LiBattery1150 : lone c15_LiBattery1150
, r_c16_LiBattery1400 : lone c16_LiBattery1400 }
{ one @r_c14_Battery.this
  let children = (r_c15_LiBattery1150 + r_c16_LiBattery1400) | one children }

sig c15_LiBattery1150
{}
{ one @r_c15_LiBattery1150.this }

sig c16_LiBattery1400
{}
{ one @r_c16_LiBattery1400.this }

fact { #c17_LowEndPhone = 0 }
abstract sig c17_LowEndPhone extends c5_MobilePhone
{}
{ no (this.@r_c6_Connectivity).@r_c12_WiFi }

abstract sig c20_HighEndPhone extends c5_MobilePhone
{ r_c26_QuadCPU : lone c26_QuadCPU }
{ (some (this.@r_c6_Connectivity).@r_c13_USB) && (some (this.@r_c6_Connectivity).@r_c12_WiFi) }

sig c26_QuadCPU
{}
{ one @r_c26_QuadCPU.this }

one sig c27_Phone1 extends c5_MobilePhone
{}
{ some (this.@r_c6_Connectivity).@r_c12_WiFi
  some (this.@r_c6_Connectivity).@r_c9_LTE
  some ((this.@r_c6_Connectivity).@r_c7_Bluetooth).@r_c3_Bluetooth21 }

one sig c34_Phone2 extends c20_HighEndPhone
{}
{ some (this.@r_c6_Connectivity).@r_c9_LTE
  some ((this.@r_c6_Connectivity).@r_c7_Bluetooth).@r_c3_Bluetooth21
  some this.@r_c26_QuadCPU }

