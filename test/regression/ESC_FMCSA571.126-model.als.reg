fact { #c1_degree = 0 }
abstract sig c1_degree
{ ref : one Int }
{ ((this.@ref) > 0) && ((this.@ref) <= 360) }

fact { #c9_Vehicle = 0 }
abstract sig c9_Vehicle
{ r_c10_DriveConfiguration : one c10_DriveConfiguration
, r_c11_ElectronicStabilityControlSystem : one c11_ElectronicStabilityControlSystem
, r_c12_AntiLockBreakSystem : one c12_AntiLockBreakSystem
, r_c13_TractionControlSystem : one c13_TractionControlSystem
, r_c14_AckermanSteerAngle : one c14_AckermanSteerAngle
, r_c24_speed : one c24_speed
, r_c25_lateralAcceleration : one c25_lateralAcceleration
, r_c35_sideSlipAngle : one c35_sideSlipAngle
, r_c45_yawRate : one c45_yawRate
, r_c55_normalYawRate : one c55_normalYawRate
, r_c66_hasOversteer : lone c66_hasOversteer
, r_c73_hasUndersteer : lone c73_hasUndersteer
, r_c80_GVWR : one c80_GVWR }
{ all disj x, y : this.@r_c14_AckermanSteerAngle | (x.@ref) != (y.@ref)
  all disj x, y : this.@r_c25_lateralAcceleration | (x.@ref) != (y.@ref)
  all disj x, y : this.@r_c35_sideSlipAngle | (x.@ref) != (y.@ref)
  all disj x, y : this.@r_c45_yawRate | (x.@ref) != (y.@ref)
  all disj x, y : this.@r_c55_normalYawRate | (x.@ref) != (y.@ref)
  ((this.((@r_c45_yawRate.@ref).@ref)) > (this.((@r_c55_normalYawRate.@ref).@ref))) <=> (some this.@r_c66_hasOversteer)
  ((this.((@r_c45_yawRate.@ref).@ref)) < (this.((@r_c55_normalYawRate.@ref).@ref))) <=> (some this.@r_c73_hasUndersteer) }

