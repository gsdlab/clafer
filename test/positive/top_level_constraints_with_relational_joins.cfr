abstract Course
	 assistants -> TA *

abstract TA
	 first -> Course ?
		[parent in this.assistants ]
	
CompilerGradStudent : TA

AIGradStudent : TA 
	
CompilerCourse : Course

MachineLearningCourse : Course
	
[CompilerGradStudent.first = CompilerCourse]

[AIGradStudent.first = MachineLearningCourse]
	
